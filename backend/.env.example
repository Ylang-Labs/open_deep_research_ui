OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
OPENROUTER_API_KEY=
TAVILY_API_KEY=
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=
LANGSMITH_TRACING=

# Only necessary for Open Agent Platform
SUPABASE_KEY=
SUPABASE_URL=
# Should be set to true for a production deployment on Open Agent Platform. Should be set to false otherwise, such as for local development.
GET_API_KEYS_FROM_CONFIG=false

# Automatic model selection based on available provider keys
# Set to false to use model strings exactly as configured without fallback
AUTO_MODEL_SELECTION=true
# Either provide a full order (takes precedence) or a single preferred provider
# PREFERRED_PROVIDER_ORDER=openai,anthropic,openrouter,google
# PREFERRED_LLM_PROVIDER=openrouter
# When true, do not fallback if the configured model's provider is unavailable
STRICT_PROVIDER_MATCH=false
